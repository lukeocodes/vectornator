{
    "name": "@lukeocodes/vectornator",
    "version": "1.0.0",
    "description": "Maintain remote vector stores with your repository content - GitHub Action and CLI tool",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "vectornator": "./dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --ext .ts --fix",
        "prepare": "husky install && npm run build",
        "prepublishOnly": "npm test && npm run lint"
    },
    "keywords": [
        "vector-store",
        "openai",
        "pinecone",
        "github-action",
        "documentation",
        "embeddings",
        "ai",
        "cli"
    ],
    "author": "Luke Oliff",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/lukeocodes/vectornator.git"
    },
    "files": [
        "dist",
        "action.yml"
    ],
    "dependencies": {
        "chalk": "^4.1.2",
        "commander": "^11.1.0",
        "dotenv": "^16.3.1",
        "glob": "^10.3.10",
        "ora": "^5.4.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.3",
        "@commitlint/config-conventional": "^18.4.3",
        "@semantic-release/commit-analyzer": "^11.1.0",
        "@semantic-release/github": "^9.2.6",
        "@semantic-release/npm": "^11.0.2",
        "@semantic-release/release-notes-generator": "^12.1.0",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.5",
        "@typescript-eslint/eslint-plugin": "^8.42.0",
        "@typescript-eslint/parser": "^6.15.0",
        "conventional-changelog-conventionalcommits": "^7.0.2",
        "eslint": "^8.56.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "semantic-release": "^22.0.12",
        "ts-jest": "^29.1.1",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}